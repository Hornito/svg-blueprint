{
  "name": "svg-blueprint",
  "version": "0.2.0",
  "description": "SVG Blueprint inspired by maker.js playground",
  "main": "./src/index.js",
  "module": "./src/index.js",
  "scripts": {
    "dev": "budo ./src/index.js -s svg-blueprint.js --dir ./demo --live --open -- -r ./src/index.js:svg-blueprint -t babelify -p licensify",
    "build:demo": "browserify -r ./src/index.js:svg-blueprint.js -t babelify -p licensify -p tinyify > ./demo/svg-blueprint.js",
    "build:tiny": "browserify -r ./src/index.js:svg-blueprint.js -t babelify -p licensify -p tinyify > ./dist/svg-blueprint.tiny.js",
    "build:http": "browserify -r ./src/index.js:svg-blueprint.js -t babelify -p licensify > ./dist/svg-blueprint.js",
    "build": "yarn build:demo && yarn build:http && yarn build:tiny",
    "format": "prettier-eslint --write ./src/**/*.js",
    "lint": "eslint ./src/**/*.js"
  },
  "homepage": "https://github.com/onlfait/svg-blueprint",
  "bugs": {
    "url": "https://github.com/onlfait/svg-blueprint/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onlfait/svg-blueprint.git"
  },
  "keywords": [
    "svg",
    "blueprint",
    "maker.js",
    "makerjs",
    "zoom",
    "pan"
  ],
  "author": "SÃ©bastien Mischler <sebastien@onlfait.ch>",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "babel-eslint": "^10.0.3",
    "babelify": "^10.0.0",
    "budo": "^11.6.3",
    "eslint": "^6.5.1",
    "licensify": "^3.1.3",
    "prettier-eslint-cli": "^5.0.0",
    "tinyify": "^2.5.1"
  }
}
